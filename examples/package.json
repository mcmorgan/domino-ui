{
  "name": "domino-ui",
  "version": "1.0.0",
  "description": "Domino UI",
  "scripts": {
    "build": "npm run cleanup && npm run copy-assets && elm make Main.elm --output=build/index.html --optimize",
    "copy-assets": "rimraf build && mkdir build && cp -r example.css build/",
    "cleanup": "rimraf elm-stuff/",
    "debug": "elm-live Main.elm --port=3030 --dir=./ -- --output=index.html --debug",
    "deploy": "npm run build && gh-pages --dist build/",
    "live": "elm-live Main.elm --port=3030 --dir=./ -- --output=index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcelmorgan/domino-ui.git"
  },
  "bugs": {
    "url": "https://github.com/marcelmorgan/domino-ui/issues"
  },
  "homepage": "https://github.com/marcelmorgan/domino-ui#readme",
  "author": "marcelmorgan",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "elm": "^0.19.1-3",
    "elm-live": "^4.0.2",
    "gh-pages": "^2.1.1",
    "rimraf": "^3.0.0"
  }
}
